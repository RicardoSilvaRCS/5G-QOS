<?xml version="1.0" encoding="utf-8"?>
<TableRow android:id="@+id/neighbor_row" xmlns:android="http://schemas.android.com/apk/res/android" android:gravity="center">
    <TextView
        android:id="@+id/txt_no"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAlignment="center"
        android:layout_weight="2"
        android:layout_marginStart="5dp"
        />
    <Space android:layout_weight="1" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/txt_tech"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAlignment="center"
        android:layout_weight="2"
        />
    <Space android:layout_weight="1" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/txt_arfcn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAlignment="center"
        android:layout_weight="2"
        />
    <Space android:layout_weight="1" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/txt_rssi_rsrp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAlignment="center"
        android:layout_weight="2"
        />
    <Space android:layout_weight="1" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/txt_cid_psc_pci"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cid_psc_pci"
        android:textAlignment="center"
        android:layout_weight="2"
        android:layout_marginEnd="5dp"
        />
</TableRow>